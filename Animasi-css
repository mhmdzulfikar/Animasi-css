    <title>Gabungan Animasi</title>
    <style>
        /* Animasi Vertikal */
        @keyframes bergerakVertikal {
            0% { transform: translateY(0); }
            25% { transform: translateY(-100px); }
            50% { transform: translateY(0); }
            75% { transform: translateY(100px); }
            100% { transform: translateY(0); }
        }

        .move-vertical {
            width: 100px;
            height: 100px;
            background-color: coral;
            animation: bergerakVertikal 3s ease-in-out infinite;
         }

        /* Animasi Bounce */
        @keyframes bounce {
            0%, 20%, 50%, 80%, 100% { transform: translateY(0); }
            40% { transform: translateY(-30px); }
            60% { transform: translateY(-15px); }
        }

        .bounce {
            width: 100px;
            height: 100px;
            background-color: royalblue;
            animation: bounce 2s ease infinite;
        }

        /* Animasi Bergerak */
        @keyframes bergerak {
            0% { transform: translateX(0); }
            25% { transform: translateX(100px); }
            50% { transform: translateX(0); }
            75% { transform: translateX(-100px); }
            100% { transform: translateX(0); }
        }

        .move {
            width: 100px;
            height: 100px;
            background-color: coral;
            animation: bergerak 3s cubic-bezier(0.25, 0.1, 0.25, 1) infinite;
        }

        /* Animasi Diagonal */
        @keyframes bergerakDiagonal {
            0% { transform: translate(0, 0); }
            25% { transform: translate(100px, 100px); }
            50% { transform: translate(0, 100px); }
            75% { transform: translate(-100px, -100px); }
            100% { transform: translate(0, 0); }
        }

        .move-diagonal {
            width: 100px;
            height: 100px;
            background-color: coral;
            animation: bergerakDiagonal 7s linear infinite;
        }

        /* Animasi Bergelombang */
        @keyframes gelombang {
            0%, 100% { transform: translateY(0); }
            25% { transform: translateY(-15px); }
            50% { transform: translateY(15px); }
            75% { transform: translateY(-15px); }
        }

        .wave {
            display: inline-block;
            width: 100px;
            height: 100px;
            background-color: goldenrod;
            animation: gelombang 1.5s ease-in-out infinite;
        }

        /* Animasi Bergetar */
        @keyframes getar {
            0%, 100% { transform: translateX(0); }
            25% { transform: translateX(-100px); }
            50% { transform: translateX(100px); }
            75% { transform: translateX(-100px); }
        }

        .shake {
            width: 100px;
            height: 100px;
            background-color: tomato;
            animation: getar 0.5s ease-in-out infinite;
        }

        /* Animasi Keluar Masuk */
        @keyframes fadeIn {
            from {opacity: 0;}
            to {opacity: 1;}
        }
        @keyframes fadeOut {
            from {opacity: 1;}
            to {opacity: 0;}
        }

        .fade {
            width: 100px;
            height: 100px;
            background-color: mediumseagreen;
            animation: fadeIn 2s ease-in-out forwards infinite, fadeOut 2s ease-in-out 2s forwards infinite; /* Fade in diikuti dengan fade out */
        }

        @keyframes flip {
            from {transform: rotateY(0);}
            to {transform: rotateY(360deg);}
        }

        .flip {
            width: 100px;
            height: 100px;
            background-color: crimson;
            animation: flip 2s linear infinite; /* Durasi 2 detik */
        }

        @keyframes putar {
            from {transform: rotate(0deg);}
            to {transform: rotate(360deg);}
        }

        .rotate {
            width: 100px;
            height: 100px;
            background-color: deepskyblue;
            animation: putar 2s linear infinite; /* Durasi 2 detik */
        }

        @keyframes kombinasi {
            0% {transform: scale(1) rotate(0deg);}
            50% {transform: scale(1.5) rotate(180deg);} /* Meningkatkan ukuran dan memutar */
            100% {transform: scale(1) rotate(360deg);} /* Kembali ke ukuran awal dan memutar penuh */
        }

        .combine {
            width: 100px;
            height: 100px;
            background-color: purple;
            animation: kombinasi 3s ease-in-out infinite; /* Durasi 3 detik */
        }

        @keyframes pulsing {
            0% {transform: scale(1);}
            50% {transform: scale(1.1);} /* Meningkatkan ukuran */
            100% {transform: scale(1);}
        }

        .pulsing {
            width: 100px;
            height: 100px;
            background-color: orange;
            animation: pulsing 1.5s ease-in-out infinite; /* Durasi 1.5 detik */
        }

        @keyframes slide {
            from {transform: translateX(-100%);}
            to {transform: translateX(0);}
        }
        .slide {
            width: 100px;
            height: 20px;
            background-color: mediumvioletred;
            animation: slide 3s ease forwards infinite; /* Durasi 1 detik */
        }
        .delay1 {animation-delay: 0s;} /* Tidak ada delay */
        .delay2 {animation-delay: 0.5s;} /* Delay 0.5 detik */
        .delay3 {animation-delay: 1s;} /* Delay 1 detik */


        .hover-effect {
            width: 100px;
            height: 100px;
            background-color: teal;
            transition: transform 0.3s ease, background-color 0.3s ease; /* Transisi */
        }
        .hover-effect:hover {
            transform: scale(1.2); /* Membesarkan saat hover */
            background-color: lightcoral; /* Mengubah warna saat hover */
        }

        @keyframes rotateScale {
            0% {transform: rotate(0deg) scale(1);}
            50% {transform: rotate(180deg) scale(1.2);}
            100% {transform: rotate(360deg) scale(1);}
        }
        .rotate-scale {
            width: 100px;
            height: 100px;
            background-color: dodgerblue;
            animation: rotateScale 2s ease infinite; /* Durasi 2 detik */
        }

        @keyframes wobble {
            0%, 100% {transform: translateX(0);}
            15% {transform: translateX(-30px);}
            30% {transform: translateX(15px);}
            45% {transform: translateX(-15px);}
            60% {transform: translateX(5px);}
        }
        .wobble {
            width: 100px;
            height: 100px;
            background-color: mediumseagreen;
            animation: wobble 1s ease infinite; /* Durasi 1 detik */
        }

        @keyframes slideIn {
            from {
                transform: translateY(-100%);
                opacity: 0; /* Mulai dari tidak terlihat */
            }
            to {
                transform: translateY(0);
                opacity: 1; /* Muncul sepenuhnya */
            }
        }
        .slide-in {
            width: 100px;
            height: 100px;
            background-color: gold;
            animation: slideIn 1s ease forwards infinite; /* Durasi 1 detik */
        }

        @keyframes rotateBounce {
            0%, 100% {transform: rotate(0deg) translateY(0);}
            50% {transform: rotate(180deg) translateY(-30px);}
        }
        .rotate-bounce {
            width: 100px;
            height: 100px;
            background-color: coral;
            animation: rotateBounce 1.5s ease infinite; /* Durasi 1.5 detik */
        }

        @keyframes colorChange {
            0% {background-color: red;}
            50% {background-color: yellow;}
            100% {background-color: blue;}
        }
        .color-change {
            width: 100px;
            height: 100px;
            animation: colorChange 3s ease infinite; /* Durasi 2 detik */
        }

        @keyframes expandContract {
            0%, 100% {transform: scale(1);}
            50% {transform: scale(1.5);}
        }

        .expand-contract {
            width: 100px;
            height: 100px;
            background-color: purple;
            animation: expandContract 1s ease infinite; /* Durasi 1 detik */
        }

        @keyframes swing {
            0%, 100% {transform: rotate(0deg);}
            15% {transform: rotate(15deg);}
            30% {transform: rotate(-10deg);}
            45% {transform: rotate(5deg);}
        }
        .swing {
            width: 100px;
            height: 100px;
            background-color: darkorange;
            animation: swing 1s ease infinite; /* Durasi 1 detik */
        }
        body {
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
            align-items: center;
            height: 100vh;
            margin: 0;
            background-color: #f0f0f0;
        }

        .animation-container {
            margin: 20px;
        }
    </style>
